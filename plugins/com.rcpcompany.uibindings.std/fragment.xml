<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any">
         <separator
               name="open"
               visible="false">
         </separator>
         <separator
               name="undo"
               visible="false">
         </separator>
         <separator
               name="new"
               visible="false">
         </separator>
         <separator
               name="delete"
               visible="false">
         </separator>
         <separator
               name="navigation"
               visible="false">
         </separator>
         <separator
               name="additions"
               visible="false">
         </separator>
         <separator
               name="select"
               visible="false">
         </separator>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=undo">
         <command
               commandId="org.eclipse.ui.edit.undo"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.redo"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=new">
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=open">
         <command
               commandId="com.rcpcompany.uibindings.commands.open"
               id="com.rcpcompany.uibindings.commands.open"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=delete">
         <command
               commandId="org.eclipse.ui.edit.cut"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.copy"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.paste"
               style="push">
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.UseDefaultValue"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=select">
         <command
               commandId="org.eclipse.ui.edit.selectAll"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=navigation">
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemToTop"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemUp"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemDown"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemToBottom"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
   <menuContribution
         locationURI="menu:navigate">
      <command
            commandId="org.eclipse.ui.navigate.backwardHistory"
            id="com.rcpcompany.uibindings.backwardHistoryMenu"
            style="pulldown">
      </command>
      <command
            commandId="org.eclipse.ui.navigate.forwardHistory"
            id="com.rcpcompany.uibindings.forwardHistoryMenu"
            style="pulldown">
      </command>
   </menuContribution>
   <menuContribution
         locationURI="toolbar:navigate">
      <command
            commandId="org.eclipse.ui.navigate.backwardHistory"
            id="com.rcpcompany.uibindings.backwardHistoryMenu"
            style="pulldown">
      </command>
      <command
            commandId="org.eclipse.ui.navigate.forwardHistory"
            id="com.rcpcompany.uibindings.forwardHistoryMenu"
            style="pulldown">
      </command>
   </menuContribution>
   <menuContribution
         class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$BackwardHistoryMenuContributor"
         locationURI="menu:com.rcpcompany.uibindings.backwardHistoryMenu">
   </menuContribution>
   <menuContribution
         class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$ForwardHistoryMenuContributor"
         locationURI="menu:com.rcpcompany.uibindings.forwardHistoryMenu">
   </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$ForwardHistoryHandler"
            commandId="org.eclipse.ui.navigate.forwardHistory">
      </handler>
      <handler
            class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$BackwardHistoryHandler"
            commandId="org.eclipse.ui.navigate.backwardHistory">
      </handler>
   </extension>
<extension
      point="org.eclipse.ui.commandImages">
   <image
         commandId="org.eclipse.ui.navigate.backwardHistory"
         icon="images/backward_nav_16x16.gif">
   </image>
   <image
         commandId="org.eclipse.ui.navigate.forwardHistory"
         icon="images/forward_nav_16x16.gif">
   </image>
</extension>

</fragment>
