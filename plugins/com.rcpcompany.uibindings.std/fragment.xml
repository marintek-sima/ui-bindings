<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<fragment>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="file"
               label="File"
               mnemonic="F">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.save"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.exit"
                  style="push">
            </command>
         </menu>
         <menu
               id="edit"
               label="Edit"
               mnemonic="E">
            <command
                  commandId="org.eclipse.ui.edit.undo"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.redo"
                  style="push">
            </command>
            <separator
                  name="paste"
                  visible="true">
            </separator>
            <command
                  commandId="com.rcpcompany.uibindings.commands.SuperPaste"
                  style="push">
            </command>
         </menu>
         <menu
               id="tools"
               label="Tools"
               mnemonic="T">
            <command
                  commandId="org.eclipse.ui.views.showView"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  style="push">
            </command>
         </menu>
         <menu
               id="navigate"
               label="Navigate"
               mnemonic="N">
         </menu>
         <menu
               id="debug"
               label="Debug"
               mnemonic="D">
         </menu>
         <menu
               id="help"
               label="Help"
               mnemonic="H">
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="file">
         </toolbar>
         <toolbar
               id="edit">
            <command
                  commandId="org.eclipse.ui.edit.undo"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.redo"
                  style="push">
            </command>
         </toolbar>
         <toolbar
               id="navigate">
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any">
         <separator
               name="open"
               visible="false">
         </separator>
         <separator
               name="new"
               visible="false">
         </separator>
         <separator
               name="delete"
               visible="false">
         </separator>
         <separator
               name="undo"
               visible="false">
         </separator>
         <separator
               name="navigation"
               visible="false">
         </separator>
         <menu
               id="popup.navigate"
               label="Navigate"
               mnemonic="N">
         </menu>
         <separator
               name="additions"
               visible="false">
         </separator>
         <separator
               name="select"
               visible="false">
         </separator>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=new">
         <menu
               label="New"
               mnemonic="N">
            <dynamic
                  class="com.rcpcompany.uibindings.internal.handlers.NewHandlerMenuContributor"
                  id="com.rcpcompany.uibindings.std.dynamics.NewMenu">
            </dynamic>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=undo">
         <command
               commandId="org.eclipse.ui.edit.undo"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.redo"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=open">
         <command
               commandId="com.rcpcompany.uibindings.commands.open"
               id="com.rcpcompany.uibindings.commands.open"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=delete">
         <command
               commandId="org.eclipse.ui.edit.cut"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.copy"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.paste"
               style="push">
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.SuperPaste"
               style="push">
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.UseDefaultValue"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=select">
         <command
               commandId="org.eclipse.ui.edit.selectAll"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:popup.navigate">
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemToTop"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemUp"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemDown"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="com.rcpcompany.uibindings.commands.moveItemToBottom"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
      </menuContribution>
   <menuContribution
         locationURI="menu:navigate">
      <command
            commandId="org.eclipse.ui.navigate.backwardHistory"
            id="com.rcpcompany.uibindings.backwardHistoryMenu"
            style="pulldown">
      </command>
      <command
            commandId="org.eclipse.ui.navigate.forwardHistory"
            id="com.rcpcompany.uibindings.forwardHistoryMenu"
            style="pulldown">
      </command>
   </menuContribution>
   <menuContribution
         locationURI="toolbar:navigatex">
      <command
            commandId="org.eclipse.ui.navigate.backwardHistory"
            id="com.rcpcompany.uibindings.backwardHistoryMenu"
            style="pulldown">
      </command>
      <command
            commandId="org.eclipse.ui.navigate.forwardHistory"
            id="com.rcpcompany.uibindings.forwardHistoryMenu"
            style="pulldown">
      </command>
   </menuContribution>
   <menuContribution
         class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$BackwardHistoryMenuContributor"
         locationURI="menu:com.rcpcompany.uibindings.backwardHistoryMenu">
   </menuContribution>
   <menuContribution
         class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$ForwardHistoryMenuContributor"
         locationURI="menu:com.rcpcompany.uibindings.forwardHistoryMenu">
   </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.rcpcompany.uibindings.handlers.EditingDomainUndoHandler"
            commandId="org.eclipse.ui.edit.undo">
         <enabledWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.canUndo">
               <equals
                     value="true">
               </equals>
            </with>
         </enabledWhen>
         <activeWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.canUndo">
               <equals
                     value="true">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.rcpcompany.uibindings.handlers.EditingDomainRedoHandler"
            commandId="org.eclipse.ui.edit.redo">
         <enabledWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.canRedo">
               <equals
                     value="true">
               </equals>
            </with>
         </enabledWhen>
         <activeWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.canRedo">
               <equals
                     value="true">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$ForwardHistoryHandler"
            commandId="org.eclipse.ui.navigate.forwardHistory">
      </handler>
      <handler
            class="com.rcpcompany.uibindings.utils.IGlobalNavigationManager$BackwardHistoryHandler"
            commandId="org.eclipse.ui.navigate.backwardHistory">
      </handler>
   </extension>
<extension
      point="org.eclipse.ui.commandImages">
   <image
         commandId="org.eclipse.ui.navigate.backwardHistory"
         icon="images/backward_nav_16x16.gif">
   </image>
   <image
         commandId="org.eclipse.ui.navigate.forwardHistory"
         icon="images/forward_nav_16x16.gif">
   </image>
</extension>
<extension
      point="org.eclipse.ui.preferencePages">
   <page
         class="com.rcpcompany.uibindings.UIBindingPreferences:basicPreferencePage"
         id="com.rcpcompany.uibindings.example.application.pages.basic"
         name="Basic UI Bindings">
   </page>
   <page
         category="com.rcpcompany.uibindings.example.application.pages.basic"
         class="com.rcpcompany.uibindings.UIBindingPreferences:validationPreferencePage"
         id="com.rcpcompany.uibindings.example.application.pages.validation"
         name="Validation">
   </page>
   <page
         class="com.rcpcompany.uibindings.navigator.internal.pages.PreferredEditorPage"
         id="com.rcpcompany.uibindings.example.application.pages.editors"
         name="Editors">
   </page>
</extension>

</fragment>
