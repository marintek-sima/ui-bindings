package no.marintek.sima.script;

public class ScriptDecoration {

	private final int length;
	private final int start;

	public ScriptDecoration(int start,int length) {
		this.start = start;
		this.length = length;
	}
	
	public int getStart() {
		return start;
	}

	public int getLength() {
		return length;
	}

}
