            class="com.rcpcompany.uibindings.UIBindingPreferences:basicPreferencePage"
            id="com.rcpcompany.uibindings.example.application.pages.basic"
            name="Basic UI Bindings">
      </page>
   </extension>
   <extension
         point="com.rcpcompany.uibindings.uiBindings">
      <bindingDecorator
            id="com.rcpcompany.uibindings.tests.bindingDecorators.loyalty"
            type="sub">
         <modelType
               class="com.rcpcompany.uibindings.tests.shop.CustomerType">
         </modelType>
         <uiType
               class="java.lang.String">
         </uiType>
         <enum
               defaultMappings="false">
            <mapping
                  model="BRONCE"
                  ui="Level 1">
            </mapping>
            <mapping
                  model="SILVER"
                  ui="Level 2">
            </mapping>
         </enum>
      </bindingDecorator>
      <bindingDecorator
            id="com.rcpcompany.uibindings.tests.bindingDecorators.loyalty"
            type="subwith">
         <modelType
               class="com.rcpcompany.uibindings.tests.shop.CustomerType">
         </modelType>
         <uiType
               class="java.lang.String">
         </uiType>
         <enum
               defaultMappings="true">
            <mapping
                  model="BRONCE"
                  ui="Level 1">
            </mapping>
            <mapping
                  model="SILVER"
                  ui="Level 2">
            </mapping>
            <mapping
                  model="SILVER"
                  ui="Level 2b">
            </mapping>
         </enum>
      </bindingDecorator>
   </extension>
   <extension
         point="com.rcpcompany.uibindings.uiBindings">
      <bindingDecorator
            type="##.#">
         <modelType
               alsoPrimitive="true"
               class="java.lang.Float">
         </modelType>
         <uiType
               class="java.lang.String">
         </uiType>
         <number
               format="%%.2f">
         </number>
      </bindingDecorator>
      <bindingDecorator
            type="limited">
         <modelType
               alsoPrimitive="true"
               class="java.lang.Float">
         </modelType>
         <uiType
               class="java.lang.String">
         </uiType>
         <number
               format="%.2f">
         </number>
         <argument
               name="min"
               value="0">
         </argument>
         <argument
               name="max"
               value="10">
         </argument>
      </bindingDecorator>
      <bindingDecorator
            type="limited">
         <modelType
               alsoPrimitive="true"
               class="java.lang.Integer">
         </modelType>
         <uiType
               class="java.lang.String">
         </uiType>
         <number
               format="%d">
         </number>
         <argument
               name="min"
               value="0">
         </argument>
         <argument
               name="max"
               value="10">
         </argument>
      </bindingDecorator>
      <bindingDecorator
            type="range">
         <modelType
               alsoPrimitive="true"
               class="java.lang.Integer">
         </modelType>
         <modelType
               alsoPrimitive="true"
               class="java.lang.Float">
