   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.rcpcompany.uibindings.extests.views.EmptyView"
            id="com.rcpcompany.uibindings.extests.views.EmptyView"
            name="Empty View"
            restorable="true">
      </view>
      <view
            allowMultiple="true"
            class="com.rcpcompany.uibindings.extests.views.TestView"
            id="com.rcpcompany.uibindings.extests.views.TestView"
            name="Test View"
            restorable="true">
      </view>
      <view
            class="com.rcpcompany.uibindings.extests.utils.ShowViewSelectionTestView"
            id="com.rcpcompany.uibindings.extests.utils.ShowViewSelectionTestView"
            name="ShowViewEnablementTestView"
            restorable="true">
      </view>
      <view
            class="com.rcpcompany.uibindings.views.ValidationView"
            id="com.rcpcompany.uibindings.views.ValidationView"
            name="Validation Status"
            restorable="true">
      </view>
   </extension>
   <extension
         point="com.rcpcompany.uibindings.uiBindings">
      <bindingDecorator
            id="com.rcpcompany.uibindings.tests.bindingDecorators.foobar"
            type="foobar">
         <argument
               name="foobar"
               value="unit-value">
         </argument>
         <uiType
               class="java.lang.String">
         </uiType>
         <modelType
               alsoPrimitive="true"
               class="java.lang.Double">
         </modelType>
         <number
               format="g">
         </number>
      </bindingDecorator>
   </extension>
   <extension
         point="com.rcpcompany.uibindings.uiBindings">
      <quickfixProcessor
            modelType="com.rcpcompany.uibindings.tests.shop.Customer"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:Shop">
      </quickfixProcessor>
      <quickfixProcessor
            feature="name"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:price">
      </quickfixProcessor>
      <quickfixProcessor
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:source"
            source="abc">
      </quickfixProcessor>
      <quickfixProcessor
            code="12345"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:code">
      </quickfixProcessor>
      <quickfixProcessor
            code="12345"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:source+code"
            source="abc">
      </quickfixProcessor>
      <quickfixProcessor
            messagePattern=".*car.*red"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:car.*red">
      </quickfixProcessor>
      <quickfixProcessor
            messagePattern=".*car.*blue"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessor:car.*blue">
      </quickfixProcessor>
      <quickfixProcessor
            messagePattern="Reference to missing processor"
            processor="com.rcpcompany.uibindings.extests.quickfixes.DummyProcessorNotExist">
      </quickfixProcessor>
   </extension>
