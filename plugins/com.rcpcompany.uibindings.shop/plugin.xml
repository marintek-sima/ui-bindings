   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.rcpcompany.uibindings.example.application.commands.ShowSelection"
            name="&amp;Show Selection">
         <commandParameter
               id="header"
               name="Header"
               optional="true">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.rcpcompany.uibindings.example.application.handlers.ShowSelection"
            commandId="com.rcpcompany.uibindings.example.application.commands.ShowSelection">
      </handler>
      <handler
            class="com.rcpcompany.uibindings.handlers.EditingDomainUndoHandler"
            commandId="org.eclipse.ui.edit.undo">
         <enabledWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.theManager">
               <test
                     forcePluginActivation="true"
                     property="com.rcpcompany.uibindings.canUndo">
               </test>
            </with>
         </enabledWhen>
         <activeWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.theManager">
               <test
                     forcePluginActivation="true"
                     property="com.rcpcompany.uibindings.canUndo">
               </test>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.rcpcompany.uibindings.handlers.EditingDomainRedoHandler"
            commandId="org.eclipse.ui.edit.redo">
         <enabledWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.theManager">
               <test
                     forcePluginActivation="true"
                     property="com.rcpcompany.uibindings.canRedo">
               </test>
            </with>
         </enabledWhen>
         <activeWhen>
            <with
                  variable="com.rcpcompany.uibindings.sourceProviders.theManager">
               <test
                     forcePluginActivation="true"
                     property="com.rcpcompany.uibindings.canRedo">
               </test>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="com.rcpcompany.uibindings.shop.handlers.SaveHandler"
            commandId="org.eclipse.ui.file.save">
      </handler>
   </extension>
