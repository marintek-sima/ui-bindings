   <extension
         point="com.rcpcompany.uibindings.uiBindings">
      <model
            class="com.rcpcompany.uibindings.tests.shop.Shop">
         <feature
               name="tmpDir">
            <standardArguments
                  label="Temporary Directory"
                  type="directoryName">
            </standardArguments>
         </feature>
         <standardArguments
               image="icons/house_16x16.png">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.Country">
         <argument
               name="openCommand"
               value="com.rcpcompany.uibindings.commands.showView(viewId=com.rcpcompany.uibindings.example.application.views.ShopBasicsView)">
         </argument>
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.CountriesObservableListFactory">
         </argument>
         <argument
               name="itemDeletor"
               value="com.rcpcompany.uibindings.items.GenericDeletor">
         </argument>
         <standardArguments
               preferredCellEditor="org.eclipse.swt.custom.CCombo">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.ShopItem">
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.ShopItemsObservableListFactory">
         </argument>
         <feature
               name="price">
            <standardArguments
                  type="price">
            </standardArguments>
         </feature>
         <argument
               name="image">
         </argument>
         <standardArguments
               image="icons\shopItem-16x16.png"
               openCommand="com.rcpcompany.uibindings.commands.showView(viewId=com.rcpcompany.uibindings.example.application.views.InventoryView)">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.Contact">
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.ContactsObservableListFactory">
         </argument>
         <feature
               name="name">
            <standardArguments
                  openCommand="com.rcpcompany.uibindings.commands.showView(viewId=com.rcpcompany.uibindings.example.application.views.ContactsView)"></standardArguments>
         </feature>
         <standardArguments
               image="icons/contact_16x16.png"
               openCommand="com.rcpcompany.uibindings.commands.showView(viewId=com.rcpcompany.uibindings.example.application.views.ContactsView)"></standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.Customer">
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.CustomersObservableListFactory">
         </argument>
         <standardArguments
               featureName="contact.name"
               openCommand="com.rcpcompany.uibindings.commands.showView(viewId=com.rcpcompany.uibindings.example.application.views.CustomersView)">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.Order">
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.OrdersObservableListFactory">
         </argument>
         <feature
               name="price">
            <standardArguments
                  type="price">
            </standardArguments>
         </feature>
         <standardArguments
               featureName="no"
               image="icons\order-16x16.png">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.OrderItem">
         <standardArguments
               featureName="item.name">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.ShopItemGroup">
         <argument
               name="validValues"
               value="com.rcpcompany.uibindings.shop.observableListFactories.ShopGroupsObservableListFactory">
         </argument>
         <standardArguments
               image="icons\group-16x16.gif"
               nullLabel="-no group-"
