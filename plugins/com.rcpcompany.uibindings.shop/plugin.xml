               preferredCellEditor="org.eclipse.swt.custom.CCombo"
               preferredControl="org.eclipse.swt.widgets.Combo">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.CustomerType">
         <feature
               name="Bronce">
            <standardArguments
                  image="icons/medal_bronze_2.png">
            </standardArguments>
         </feature>
         <feature
               name="Silver">
            <standardArguments
                  image="icons/medal_silver_2.png">
            </standardArguments>
         </feature>
         <feature
               name="Gold">
            <standardArguments
                  image="icons/medal_gold_1.png">
            </standardArguments>
         </feature>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.ShopItemURL">
         <standardArguments
               featureName="url">
         </standardArguments>
      </model>
      <model
            class="com.rcpcompany.uibindings.tests.shop.ShopItemDescription">
         <standardArguments
               featureName="description">
         </standardArguments>
      </model>
