<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.rcpcompany.uibindings.tests.utils.views.EmptyView"
            id="com.rcpcompany.uibindings.extests.views.EmptyView"
            name="Empty View"
            restorable="false">
      </view>
      <view
            allowMultiple="true"
            class="com.rcpcompany.uibindings.tests.utils.views.UIBTestView"
            id="com.rcpcompany.uibindings.extests.views.TestView"
            name="Test View"
            restorable="false">
      </view>
   </extension>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.rcpcompany.uibindings.tests.utils.application.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.rcpcompany.uibindings.tests.utils.application.Perspective"
            id="com.rcpcompany.uibindings.tests.utils.perspectives.TestPerspective"
            name="Test Perspective">
      </perspective>
   </extension>

</plugin>
