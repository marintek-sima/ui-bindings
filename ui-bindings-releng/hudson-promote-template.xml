<hudson.plugins.promoted__builds.PromotionProcess>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <conditions>
    <hudson.plugins.promoted__builds.conditions.ManualCondition/>
  </conditions>
  <icon>star-blue</icon>
  <buildSteps>
    <hudson.tasks.Ant>
      <targets>promote_to_stable</targets>
      <buildFile>releng/build.xml</buildFile>
			<properties>builder=$WORKSPACE/releng
buildDirectory=$WORKSPACE
buildfile=BUILDFILE
launcherJar=LAUNCHER_JAR
baseLocation=BASE_LOCATION
gcLogin=GC_LOGIN
gcPassword=GC_PASSWORD
ant_libs=ANT_LIBS
repoPath=REPO_PATH</properties>
    </hudson.tasks.Ant>
  </buildSteps>
</hudson.plugins.promoted__builds.PromotionProcess>